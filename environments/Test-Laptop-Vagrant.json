{
  "name": "Test-Laptop-Vagrant",
  "override_attributes": {
    "bcpc": {
      "vagrant": {
        "nat_ip": "10.0.2.15"
      },
      "virt_type" : "qemu",
      "enabled": {
        "logging": false,
        "monitoring": false,
        "metrics": false,
        "dns": true,
        "secure_fixed_networks" : false
      },
      "ceph": {
        "pgp_auto_adjust" : true,
        "hdd_disks" : [ "sdb", "sdc" ],
        "ssd_disks" : [ "sdd", "sde" ],
        "chooseleaf" : "rack"
      },
      "graphite": {
        "max_creates_per_min": "inf"
      },
      "nova" : {
        "ram_allocation_ratio" : "20.0",
        "cpu_allocation_ratio": "20.0"
      },
      "keystone": {
        "default_domain": "local"
      },
      "domain_name" : "bcpc.example.com",
      "management": {
        "vip" : "10.0.100.5",
        "interface" : "eth1",
        "netmask" : "255.255.255.0",
        "cidr" : "10.0.100.0/24",
        "gateway" : "10.0.100.3"
      },
      "monitoring": {
        "vip" : "10.0.100.6"
      },
      "storage": {
        "interface" : "eth2",
        "netmask" : "255.255.255.0",
        "cidr" : "172.16.100.0/24",
        "gateway" : "172.16.100.3"
      },
      "floating": {
        "vip" : "192.168.100.5",
        "interface" : "eth3",
        "netmask" : "255.255.255.0",
        "cidr" : "192.168.100.0/24",
        "gateway" : "192.168.100.3",
        "available_subnet" : "192.168.100.128/25"
      },
      "additional_floating": [
        {
          "cidr": "192.168.200.0/24",
          "netmask": "255.255.255.0",
          "ip_pools" : [
            "192.168.200.128/25"
          ]
        }
      ],
      "bootstrap": {
        "interface" : "eth0",
        "pxe_interface" : "eth1",
        "server" : "10.0.100.3",
        "dhcp_subnet" : "10.0.100.0",
        "dhcp_range" : "10.0.100.14 10.0.100.250"
      },
      "dns_servers" : [ "8.8.8.8", "8.8.4.4" ]
    },
    "ntp": {
      "servers" : [ "0.pool.ntp.org", "1.pool.ntp.org", "2.pool.ntp.org", "3.pool.ntp.org" ]
    },
    "bcpc-extra": {
      "postfix": {
        "enabled": false
      }
    },
    "chef_client": {
      "server_url": "http://10.0.100.3:4000",
      "cache_path": "/var/chef/cache",
      "backup_path": "/var/chef/backup",
      "validation_client_name": "chef-validator",
      "run_path": "/var/chef"
    }
  },
  "json_class": "Chef::Environment",
  "description": "Laptop test environment",
  "cookbook_versions": {
  },
  "chef_type": "environment"
}
