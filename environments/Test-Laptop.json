{
    "name": "Test-Laptop",
    "override_attributes": {
	    "bcpc": {
            "routemon" : {
                "numfixes" : "10000"
            },
	        "hypervisor_domain" : "local.lan",
	        "cluster_domain" : "bcpc.local.lan",
            "virt_type" : "qemu",
            "enabled": {
		        "logging": false,
                "monitoring": false,
                "metrics": false,
                "dns": true,
                "secure_fixed_networks" : false
            },
            "ceph": {
                "pgp_auto_adjust" : true,
                "hdd_disks" : [ "sdb", "sdc" ],
                "ssd_disks" : [ "sdd", "sde" ],
                "chooseleaf" : "host"
            },
            "nova" : {
                "ram_allocation_ratio" : "20.0",
                "cpu_allocation_ratio": "20.0",
                "nova_single_cpu": true
            },
            "management": {
                "vip" : "10.0.100.5",
                "interface" : "eth0",
                "netmask" : "255.255.255.0",
                "cidr" : "10.0.100.0/24",
                "gateway" : "10.0.100.3",
                "monitoring": {
                    "vip" : "10.0.100.6"
                }
            },
            "storage": {
                "interface" : "eth1",
                "netmask" : "255.255.255.0",
                "cidr" : "172.16.100.0/24",
                "gateway" : "172.16.100.3"
            },
            "floating": {
                "vip" : "192.168.100.5",
                "interface" : "eth2",
                "netmask" : "255.255.255.0",
                "cidr" : "192.168.100.0/24",
                "gateway" : "192.168.100.3",
                "available_subnet" : "192.168.100.128/25"
            },
            "bootstrap": {
                "interface"     : "eth0",
                "pxe_interface" : "eth1",
                "server"        : "10.0.100.3",
                "dhcp_subnet"   : "10.0.100.0",
                "dhcp_range"    : "10.0.100.14 10.0.100.250",
                "mirror"        : "10.0.100.2",
                "mirror_path"   : "/ubuntu"
            },
            "repos": {
                "elasticsearch": "http://10.0.100.2/elasticsearch",
                "erlang"       : "http://10.0.100.2/erlang",
                "fluentd"      : "http://10.0.100.2/fluentd",
                "haproxy"      : "http://10.0.100.2/haproxy",
                "percona"      : "http://10.0.100.2/percona",
                "openstack"    : "http://10.0.100.2/openstack",
                "rabbitmq"     : "http://10.0.100.2/rabbitmq",
                "ceph"         : "http://10.0.100.2/ceph",
                "zabbix"       : "http://10.0.100.2/zabbix"
            },
            "dns_servers" : [ "8.8.8.8", "8.8.4.4" ]
        },
        "ntp": {
            "servers" : [ "time.apple.com" ]
        },
        "chef_client": {
            "server_url": "http://10.0.100.3:4000",
            "cache_path": "/var/chef/cache",
            "backup_path": "/var/chef/backup",
            "validation_client_name": "chef-validator",
            "run_path": "/var/chef"
        },
        "ubuntu": {
            "archive_url": "http://10.0.100.2/ubuntu",
            "include_source_packages": false,
            "security_url": "http://10.0.100.2/ubuntu"
        }
    },
    "json_class": "Chef::Environment",
    "description": "Laptop test environment",
    "cookbook_versions": {
    },
    "chef_type": "environment"
}
